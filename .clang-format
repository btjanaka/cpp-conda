BasedOnStyle: Google
IncludeCategories:
  # Headers that do not come from stdlib
  - Regex:           '^<.*/.*>'
    Priority:        3
    SortPriority:    0
  # C stdlib headers
  - Regex:           '^<.*\.h>'
    Priority:        1
    SortPriority:    0
  # C++ stdlib headers
  - Regex:           '^<.*'
    Priority:        2
    SortPriority:    0
  # Everything else
  - Regex:           '.*'
    Priority:        4
    SortPriority:    0
